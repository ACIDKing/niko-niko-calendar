{
  "name": "niko-niko-calendar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "@rebass/grid": "6.0.0-7",
    "lodash": "^4.17.21",
    "ramda": "0.26.1",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-scripts": "2.1.5",
    "react-select": "2.4.0",
    "recharts": "1.5.0",
    "serve": "10.1.2",
    "styled-components": "4.1.3",
    "uniqid": "5.0.3"
  },
  "devDependencies": {
    "prettier": "1.16.4",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "jest-enzyme": "7.0.1",
    "jest-styled-components": "6.3.1"
  },
  "scripts": {
    "start-dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "jsxBracketSameLine": true
  },
  "jest": {
    "coverageReporters": [
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.d.ts",
      "!node_modules/",
      "!scripts/"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
